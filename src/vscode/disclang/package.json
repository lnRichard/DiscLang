{
    "name": "disclang",
    "displayName": "DiscLang",
    "description": "DiscLang Programming Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "disclang",
            "aliases": ["DiscLang", "disclang"],
